#include<stdio.h>
#include<locale.h>
#include<math.h>

/*Faça um programa que le dois numeros inteiros positivos a e b. Utilizando laços, o seu programa deve calcular e imprimir o valor a elevado a b.*/

int main (){
	int a, b, acu=1, i=1;
	
	printf("digite um numero:\n");
	scanf("%d", &a);
	
	printf("digite o numero elevado:\n");
	scanf("%d", &b);
	
	while(i<=b) {
		acu*=a;
		i++;
	}
	printf("%d", acu);
	
	return 0;
}
