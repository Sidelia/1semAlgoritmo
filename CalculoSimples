#include <stdio.h>

int main()
{
    int cod=0, qtd=0;
    double valor =0, total=0;

    scanf("%d %d %lf", &cod, &qtd, &valor);
	
    total += qtd*valor;
     
    scanf("%d %d %lf", &cod, &qtd, &valor);

    total += (qtd*valor); 

    printf("VALOR A PAGAR: R$ %.2lf\n", total);

    return 0;
}
