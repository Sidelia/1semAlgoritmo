int main() {
	
	int a;
	
	printf("Digite o RA:");
	scanf("%d", &a);
	
	switch(a) {
	case 10129:
		printf("MAria Candida Moreira Telles\n");
		break;
		
	case 33860:
		printf("Larissa Garcia Alfonsi\n");
		break;
		
	default:
		printf("O aluno nao esta matriculado\n");
		}
}
