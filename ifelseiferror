int main (){
	
	float a;
		
	printf("Digite o valor da venda: ");
		scanf("%f", &a);
		
	if(a <= 2500.00);
		else(a*0.017);
		 printf("Valor da comissão: %d");		 
	
	else if(a >= 250000.01 <= 6250.00);
		else(a*0.066);
		printf("Valor da comissão: %d");
	
		
	
	
